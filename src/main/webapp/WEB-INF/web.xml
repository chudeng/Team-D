  </servlet>
  
  <servlet-mapping>
  	<servlet-name>MemberController</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>FeedsController</servlet-name>
  	<url-pattern>*.bo</url-pattern>
  </servlet-mapping>
